{
  "name": "@kineticdata/react",
  "version": "0.1.0-RC10",
  "description": "Kinetic Data React component library.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "yarn dir && yarn lint && yarn assets && yarn js",
    "build:watch": "yarn dir && yarn lint && yarn assets && concurrently 'yarn lint:watch' 'yarn assets:watch' 'yarn js:watch'",
    "dir": "rimraf lib && mkdir -p lib && mkdir -p lib/assets",
    "js": "cross-env NODE_ENV=production babel src --out-dir lib --ignore \"**/*.test.js\"",
    "js:watch": "cross-env NODE_ENV=production babel src --watch --out-dir lib --ignore \"**/*.test.js\"",
    "assets": "cp -r src/assets/* lib/assets",
    "assets:watch": "onchange 'src/assets/**/*' -- yarn assets",
    "lint": "eslint src --ext .js",
    "lint:watch": "onchange 'src/**/*.js' -- eslint {{changed}}",
    "start": "styleguidist server",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "styleguide:build": "styleguidist build",
    "format": "prettier --write \"**\""
  },
  "dependencies": {
    "actiontyper": "^0.0.4",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "deepequal": "^0.0.1",
    "husky": "^1.3.1",
    "immutable": "4.0.0-rc.9",
    "jest-immutable-matchers": "^2.0.1",
    "lodash.isplainobject": "^4.0.6",
    "lodash.isstring": "^4.0.1",
    "modularscale-sass": "^3.0.8",
    "moment": "^2.21.0",
    "prop-types": "^15.7.2",
    "react-autosuggest": "^9.4.3",
    "react-avatar": "^3.5.0",
    "react-dropzone": "^4.2.3",
    "react-markdown": "^4.0.6",
    "react-redux": "^6.0.0",
    "reactstrap": "^7.1.0",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.1"
  },
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "babel-eslint": "9.x",
    "babel-jest": "^24.5.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react-app": "^7.0.2",
    "bootstrap": "4.1.3",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "documentation": "^9.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "5.x",
    "eslint-config-react-app": "^3.0.8",
    "eslint-plugin-flowtype": "2.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.x",
    "faker": "^4.1.0",
    "font-awesome": "4.7.0",
    "jest": "^23.0.0",
    "lint-staged": "^8.1.3",
    "onchange": "^5.2.0",
    "prettier": "^1.16.4",
    "react": "^16.8.1",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.8.1",
    "react-scripts": "^2.1.8",
    "react-styleguidist": "^9.0.0"
  },
  "author": "Kinetic Data, Inc",
  "homepage": "https://github.com/kineticdata/react-kinetic-lib",
  "license": "MIT",
  "repository": "https://github.com/kineticdata/react-kinetic-lib",
  "keywords": [
    "react-component",
    "kinetic data",
    "kinops"
  ],
  "lint-staged": {
    "**": [
      "prettier --write",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      [
        "react-app",
        {
          "absoluteRuntime": false
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
