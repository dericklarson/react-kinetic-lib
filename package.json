{
  "name": "react-kinetic-lib",
  "version": "1.0.0",
  "description": "react-kinetic-lib React component",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component",
    "clean": "nwb clean-module && nwb clean-demo",
    "prepublishOnly": "npm run build",
    "start": "nwb serve-react-demo",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server",
    "format": "prettier --write \"**\"",
    "precommit": "lint-staged",
    "build:watch": "nodemon -w src -x 'rm .babelrc &> /dev/null; nwb build-react-component --no-demo'"
  },
  "dependencies": {
    "husky": "^1.3.1",
    "immutable": "4.0.0-rc.9",
    "lint-staged": "^8.1.3",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.1"
  },
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "nwb": "0.23.x",
    "react": "^16.8.1",
    "react-dom": "^16.8.1"
  },
  "author": "",
  "homepage": "",
  "license": "MIT",
  "repository": "",
  "keywords": [
    "react-component"
  ],
  "lint-staged": {
    "**": [
      "prettier --write",
      "git add"
    ]
  }
}
